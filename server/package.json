{
    "name": "server",
    "version": "1.0.0",
    "main": "server.ts",
    "license": "MIT",
    "scripts": {
        "watch": "tsc -w",
        "dev": "cross-env NODE_DEV=true nodemon -r dotenv/config dist/index.js",
        "start": "ts-node src/index.ts",
        "test": "cross-env NODE_DEV=true NODE_ENV=test ts-mocha tests/**/*.test.ts --package package.json",
        "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert"
    },
    "dependencies": {
        "@azure/storage-blob": "^12.8.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/express-graphql": "^0.9.0",
        "@types/graphql": "^14.5.0",
        "apollo-server-express": "^3.5.0",
        "argon2": "^0.28.3",
        "class-validator": "^0.13.2",
        "connect-redis": "^6.0.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-graphql": "^0.12.0",
        "express-session": "^1.17.2",
        "graphql": "^15.6.1",
        "graphql-upload": "^13.0.0",
        "into-stream": "^7.0.0",
        "ioredis": "^4.28.2",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.7.1",
        "redis": "^4.0.1",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typedi": "^0.10.0",
        "typeorm": "0.2.38",
        "typeorm-naming-strategies": "^2.0.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/connect-redis": "^0.0.18",
        "@types/cookie-parser": "^1.4.2",
        "@types/express-session": "^1.17.4",
        "@types/graphql-upload": "^8.0.7",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/mocha": "^9.0.0",
        "@types/node": "^8.0.29",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.3",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "chai": "^4.3.4",
        "dotenv": "^10.0.0",
        "eslint": "^8.4.1",
        "mocha": "^9.1.3",
        "nodemon": "^2.0.13",
        "supertest": "^6.1.6",
        "ts-mocha": "^8.0.0",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.2"
    },
    "mocha": {
        "require": [
            "ts-node/register/transpile-only",
            "tsconfig-paths/register",
            "dotenv/config",
            "reflect-metadata"
        ],
        "recursive": true,
        "exit": true,
        "timeout": 99999,
        "forbidPending": true,
        "color": true
    }
}
